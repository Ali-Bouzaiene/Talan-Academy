<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <servlet>
    <servlet-name>vintud</servlet-name>
    <servlet-class>com.octest.servlets.vintud</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>vintud</servlet-name>
    <url-pattern>/vintud/create/announcement</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>announcements</servlet-name>
    <servlet-class>com.octest.servlets.announcements</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>announcements</servlet-name>
    <url-pattern>/vintud/announcements</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>SignInController</servlet-name>
    <servlet-class>com.octest.servlets.SignInController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignInController</servlet-name>
    <url-pattern>/vintud/signIn</url-pattern>
  </servlet-mapping>
</web-app>